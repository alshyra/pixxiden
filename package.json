{
  "name": "pixiden-frontend",
  "version": "0.1.0",
  "description": "PixiDen - A cozy game library launcher for Linux",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:appimage": "tauri build --target appimage",
    "postinstall": "node scripts/postinstall.js",
    "setup:sidecars": "bash scripts/download-sidecars.sh",
    "setup:sidecars:legacy": "bash scripts/setup-sidecars.sh",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "wdio run wdio.conf.ts",
    "test:e2e:headless": "wdio run wdio.conf.ts --headless",
    "test:build": "npm run tauri:build && npm run test:e2e"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-log": "^2.8.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.5",
    "lucide-vue-next": "^0.309.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.6",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@wdio/cli": "^9.23.2",
    "@wdio/local-runner": "^9.23.2",
    "@wdio/mocha-framework": "^9.23.2",
    "@wdio/spec-reporter": "^9.23.2",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "happy-dom": "^20.3.4",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^4.0.17",
    "vue-tsc": "^1.8.27",
    "webdriverio": "^9.23.2"
  }
}